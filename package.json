{
  "license": "MIT",
  "scripts": {
    "client-install": "cd client && npm install",
    "server-install": "cd server && npm install",
    "start-client": "cd client && npm run start",
    "start-client:dev": "cd client && npm run start",
    "start-server": "cd server && npm run start",
    "start-server:dev": "cd server && npm run start:dev",
    "install-all-deps": "concurrently \"npm run server-install\" \"npm run client-install\"",
    "start:dev": "concurrently \"npm run start-client:dev\" \"npm run start-server:dev\"",
    "test:server": "cd server && yarn test",
    "test:client": "cd client && yarn test",
    "test:watch:server": "cd server && yarn test:watch",
    "test": "concurrently \"yarn test:client\" \"yarn test:server\"",
    "test:watch": "concurrently \"yarn test:client\" \"yarn test:watch:server\"",
    "start":"npm run start-server"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.9.3",
    "@types/react-share": "^4.0.0",
    "concurrently": "^7.2.2",
    "react-qr-code": "^2.0.7",
    "react-share": "^4.4.0"
  }
}
